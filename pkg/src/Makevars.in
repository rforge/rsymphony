SYMPHONY_LIBS = -lSym -lCgl -lOsiClp -lClp -lOsi -lCoinUtils

PKG_CPPFLAGS = @SYMPHONY_INCLUDE_PATH@
PKG_LIBS = @SYMPHONY_LIB_PATH@ $(SYMPHONY_LIBS) $(LAPACK_LIBS) \
           $(BLAS_LIBS) $(FLIBS)

.PHONY: all @SYMPHONY_TS@

all: @SYMPHONY_TS@ $(SHLIB)
$(SHLIB): @SYMPHONY_TS@

SYMPHONY.ts:
	@(cd SYMPHONY && make && make install)
	touch $@
